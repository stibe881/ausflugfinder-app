    "react-native-svg": "15.12.1",
    "react-native-tab-view": "^4.2.2",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "supercluster": "^8.0.1",
    "superjson": "^1.13.3",
    "yaml": "^2.8.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.21",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^22.8.1",
    "@types/qrcode": "^1.5.6",
    "@types/react": "~19.1.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.18.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "qrcode": "^1.5.4",
    "tsx": "^4.19.1",
    "typescript": "~5.9.2",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@9.12.0"
}
